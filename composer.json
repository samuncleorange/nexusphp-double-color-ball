{
    "name": "samuncleorange/nexusphp-double-color-ball",
    "description": "Double Color Ball lottery plugin for NexusPHP - A fun lottery game using magic points (seed bonus)",
    "type": "library",
    "license": "GPL-2.0-only",
    "keywords": [
        "nexusphp",
        "plugin",
        "lottery",
        "double-color-ball"
    ],
    "authors": [
        {
            "name": "samuncleorange",
            "email": "samuncleorange@example.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "laravel/framework": "^12.0",
        "filament/filament": ">=3.0 <6.0"
    },
    "autoload": {
        "psr-4": {
            "NexusPlugin\\DoubleColorBall\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NexusPlugin\\DoubleColorBall\\DoubleColorBallServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}